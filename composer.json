{
    "name": "sergeykoz/yii2-livecontent",
    "version":"0.1.0",
    "description": "Easy editing site content for the Yii framework",
    "keywords": ["yii2", "edit", "content"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Kozin",
            "email": "sergagame1@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "2amigos/yii2-ckeditor-widget" : "~1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ssoft\\livecontent\\": "src"
        }
    },
    "extra": {
        "config-plugin": {            
            "web": [
                "src/config/web.php"
            ],
            "console": [
                "src/config/console.php"
            ]
        }
    }
}
